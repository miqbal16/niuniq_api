{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "linebreak-style": 0,
        "no-console": 0,
        "no-unused-vars": 0,
        "consistent-return": 0,
        "func-names": 0,
        "no-underscore-dangle": 0,
        "prefer-arrow-callback": 0,
        "no-param-reassign": 0,
        "prefer-const": 0,
        "object-curly-newline": 0,
        "no-return-await": 0,
        "no-loop-func": 0,
        "no-await-in-loop": 0,
        "no-useless-escape": 0,
        "no-use-before-define": 0,
        "prefer-destructuring": 0,
        "arrow-body-style": 0,
        "no-shadow": 0
    }
}
